@model IEnumerable<NAMIS.Models.languages>
    <p>
        <a class="btn btn-outline-success" asp-action="Index" asp-area="" asp-controller="particularofchildrens"><i class="fas"></i></a>

        <a class="btn btn-outline-success" asp-action="Index" asp-area="" asp-controller="dpqs"><i class="fa"></i></a>

    </p>

<div class="row">
    <div style="height: 100%;width: 100%;overflow-y: scroll;  overflow-x: auto" class="col-xs-12">
        <div class="table-responsive">
            <table id="myTable" class="table table-hover">
                <thead>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.Language)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Spoken)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Written)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.ExamQualified)
                        </th>
                       

                        <th>
                            @Html.DisplayNameFor(model => model.SprpNo)
                        </th>
                        <th>
                            <a onclick="showInPopup('@Url.Action("CreateEdit","languages",new {ID=0},Context.Request.Scheme)','New Record')" class="btn btn-success text-white"><i class="fas fa-random"></i> New Record</a>
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.Language)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Spoken)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Written)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ExamQualified)
                            </td>
                           

                            <td>
                                @Html.DisplayFor(modelItem => item.SprpNo)
                            </td>
                            <td>
                                <div>
                                    <a onclick="showInPopup('@Url.Action("CreateEdit","languages",new {id=item.ID},Context.Request.Scheme)','Update Record')" class="btn btn-info text-white"><i class="fas fa-pencil-alt"></i> Edit</a>
                                    <a asp-action="Details" class="btn btn-danger btn-rounded btn-sm my-0" asp-route-id="@item.ID">Details</a> |
                                    <form asp-action="Delete" asp-route-id="@item.ID" onsubmit="return jQueryAjaxDelete(this)" class="d-inline">
                                        <input type="hidden" asp-for="@item.ID" />
                                        <input type="submit" value="Delete" class="btn btn-danger" />
                                    </form>
                                </div>
                               
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
