@model NAMIS.Models.schoolcertificatehelds

@{
    Layout = null;
}

<div class="row">
    <div class="col-md-12">
        <form asp-action="CreateEdit" asp-route-id="@Model.Id" onsubmit="return jQueryAjaxPost(this);">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="schoolcertificateheld" class="control-label"></label>
                <select asp-for="schoolcertificateheld" data-val-required="school certificate held Required" class="form-control">


                    <option value="">---Select---</option>
                    <option value="Phd">Phd</option>
                    <option value="Msc">Msc</option>
                    <option value="Msc Ed">Msc Ed</option>
                    <option value="M Ed">M Ed</option>
                    <option value="M Tech">M Tech</option>
                    <option value="M Eng">M Eng</option>
                    <option value="MBA">MBA</option>
                    <option value="MBA Ed">MBA Ed</option>
                    <option value="B.sc">B.sc</option>
                    <option value="B.sc Ed">B.sc Ed</option>
                    <option value="B Ed">B Ed</option>
                    <option value="BA">BA</option>
                    <option value="BA Ed">BA Ed</option>
                    <option value="B Tech">B Tech</option>
                    <option value="B Eng">B Eng</option>
                    <option value="HND">HND</option>
                    <option value="ND">ND</option>
                    <option value="OND">OND</option>
                    <option value="NCE">NCE</option>
                    <option value="Diploma">Diploma</option>
                    <option value="SSCE">SSCE</option>
                    <option value="Certification">Certification</option>
                    <option value="Certificate">Certificate</option>
                    <option value="FSLC">FSLC</option>

                    <option value="Others">Others</option>
                    
                </select>
                <span asp-validation-for="schoolcertificateheld" class="text-danger"></span>
            </div>
           


            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <div class="col-md-6 offset-md-3">
                    <input type="submit" id="btnSubmit" value="Save" class="btn btn-primary btn-block" />
                </div>
            </div>
        </form>
    </div>
</div>

