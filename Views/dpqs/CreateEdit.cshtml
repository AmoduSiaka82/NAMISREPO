@model NAMIS.Models.dpq

@{
    Layout = null;
}



    

<div class="row">
    <div class="col-md-12">
        <form asp-action="CreateEdit" asp-route-id="@Model.Id" onsubmit="return jQueryAjaxPost(this);">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                
                <div class="form-group">
                    <label asp-for="Dpq" class="control-label"></label>
                    <select asp-for="Dpq" class="form-control" asp-items="ViewBag.Dpqs">
                        <option value="">---Select---</option>

                    </select>
                    <span asp-validation-for="Dpq" class="text-danger"></span>
                </div>
                <span asp-validation-for="Dpq" class="text-danger"></span>
            </div>
         

           

            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <div class="col-md-6 offset-md-3">
                    <input type="submit" id="btnSubmit" value="Save" class="btn btn-primary btn-block" />
                </div>
            </div>
        </form>
    </div>
</div>


